h3, h4 {font-size: 14px;}

body {
margin: 0;
background: url(../images/gradient.jpg) 0 0 repeat-x #666;
font-size: 12px;
font-family: Verdana, Arial, Helvetica, sans-serif;
padding: 0;
}

a {
    color: #666;
}

#wrap {
background: url(../images/topgradient.jpg) top center no-repeat;
width: 785px;
height: 600px;
margin: auto;
}

/* header */
#header {}

#logo {
float: left;
margin: 0 0 0 15px; /*0 0 0 25px*/
}

#phone {
float: right;
margin: 0 15px 0 0; /* was 42*/
}

#banner {
clear: both;
background: url(../images/banner.jpg) 0 0 no-repeat;
width: 777px;
height: 150px; /*206*/
margin: auto;
}

#flash {
width: 705px;/*737*/
height: 130px;/*182*/
background: url(../images/flash.jpg) 0 0 no-repeat;
position: relative;
top: 9px;
left: 36px;/*20*/
}

/* header */
#topnav {
clear: right;
float: right;
height: 21px;
/*
background: url(images/topnav.jpg) 0 0  no-repeat;
*/
margin: 0 12px 0 0;
}

#topnav ul {
float: left;
width:400px;
height: 21px;
margin: -10px 0 0 0;
padding:0;
}

#topnav li {
list-style: none;
display: block;
float: right;
height: 31px;
text-align: center;
background: url(../images/navtabs1.gif) left top no-repeat;
margin-right: 15px;
}

#topnav li:hover {
color: #000;
}

#topnav li a{
display: block;
height: 21px;
text-align: center;
background: url(../images/navtabs2.gif) right top no-repeat;
color: #666;
text-decoration: none;
padding: 10px 1em 0 1em;
float: left;
margin-right: -10px;
}

#topnav li a:hover{
color: #000;
}

#imageHeader{
    background-color:white;
    width:100%;
    text-align:center;
}

.first {
margin: 0 0 0 38px;
}


#breadcrumb {   
width: 777px;
height: 30px;
margin: auto;
background: url(../images/breadcru.jpg) 0 0 repeat-x;
color: #808080;
}

#breadcrumb p {
font-weight: bold;
float: left;
margin-left: 12px;
}

#breadcrumb p a{
color: #666;
text-decoration: none;
}

#breadcrumb p a:hover{
color: #000;
}

/* main */
#main {
background: #fff;
width: 777px;
margin: auto;
}

#content {
float: left;
width: 470px;
margin: 14px 0 0 12px;
}

#sidebar {
float: right;
width: 275px;
margin: 14px 10px 0 0;
}
#sidebar h4{
    height:17px;
}


.contentitem {
background: url(../images/contenti.jpg) 0 0 no-repeat #fff;
width: 466px;
float: left;
margin-bottom: 30px;
overflow: hidden;
padding-left:10px;
}

#content h5 {
float: right;
color: #999;
margin: 10px 20px 0 0;
font-weight: bold;
clear: both;
width: 80%;
text-align: right;
}

#content h1 {
clear: right;
font-weight: bold;
color: #808080;
margin: 0 0 10px 10px;
font-size:1.25em;
}

#content h1 strong {
color: #000;
font-weight: normal;
font-size: 2em;
vertical-align: -.25em;
}

.item{
    clear:both;
    width:90%;
margin-left:10px;
}

.number{
    width:50px;
}


.sum{

    text-align: right;
   border-top: 1px solid #308fbf;
}

#content p {
display: block;
width: 417px;
margin: 0 0 0 10px;
color: #333;
/*line-height: 1.5em;*/
clear: left; 
}

.contentitem table {
 	font-size: 100%;
 	width: 90%;
 	border-collapse: collapse;
        margin-left:10px;
}
 	
 .contentitem table td
 	{
 	border-top: 1px solid #fff;
 	padding: 3px;
 	background: #fbfbfb;
 	vertical-align: top;
 	}
 	
.contentitem table th
 	{
 	padding: 6px 3px;	
 	color: #333;
 	background: #f3f3f3;
 	vertical-align: top;
 	}
th {
text-align:left;
}

th .amount{
    text-align:right;
}
.amount{
    text-align:right;
}
.contentitem table th .amount{
    text-align:right;
}

