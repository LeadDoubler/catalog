/* FROM NVC */

.contentitem form
{
border: 1px solid #e3e3e3;
margin-right: 20px;
padding: 10px;
background: #fbfbfb;
}

.contentitem form h4
{
margin-top: 0px;
padding-top: 0px;
}


.contentitem form div
{
clear: both;
margin: 0;
padding-top: 10px;
}

.contentitem form em
{
font-style: normal;
color: #003c60;
}

.form_text
{
width: 350px;
padding: 2px;
font-family: Lucida, Lucida Grande, Verdana, sans-serif;
font-size: 95%;
border: 1px solid #c7c7c7;
}

.form_select
{
padding: 2px;
font-family: Lucida, Lucida Grande, Verdana, sans-serif;
font-size: 95%;
border: 1px solid #c7c7c7;
}

.form_textarea
{
width: 350px;
padding: 2px;
font-family: Lucida, Lucida Grande, Verdana, sans-serif;
font-size: 95%;
border: 1px solid #c7c7c7;
}

.contentitem form p
{
margin: 5px 0;
padding-left: 135px;
}

/* Forms */
input, textarea {
width:90%;
margin:2px;
background: url(../images/forms000.jpg) 0 0 repeat-x #fff;
border:2px solid #e6ecf2;
padding: .2em;
color: #666;
}
form{
    clear:both;
}

.submit {
background: #fff;
border: #c3ced9 2px solid;
float: left;
padding: .1em;
clear: both;
}

.submit:hover {
color: #000;
border: #b6d4f2 2px solid;
}

fieldset {
margin: 25px auto 0 auto;
width:80%;
border: none;
overflow: hidden;
}

label {
font-weight:bold;
color: #003366;
font-size:1em;
margin: 0;
width:100%;
}

