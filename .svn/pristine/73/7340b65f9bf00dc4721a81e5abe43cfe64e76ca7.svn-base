package com.asap.security.resourse;

import com.asap.security.Role;

public class SecurityResource {

	private String url;

	private Role role;

	private transient Role defaultRole;

	public SecurityResource(String url, Role role, Role defaultRole) {
		super();
		this.url = url;
		this.role = role;
		this.defaultRole = defaultRole;
	}

	public Role getDefaultRole() {
		return defaultRole;
	}

	public void setDefaultRole(Role defaultRole) {
		this.defaultRole = defaultRole;
	}

	public Role getRole() {
		return role;
	}

	public void setRole(Role role) {
		this.role = role;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}
}
