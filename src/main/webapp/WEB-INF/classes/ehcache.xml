<?xml version="1.0" encoding="UTF-8"?>



<ehcache>
   <diskStore path="java.io.tmpdir"/>
   <defaultCache
       maxElementsInMemory="500"
       eternal="false"
       timeToIdleSeconds="300"
       timeToLiveSeconds="300"
       overflowToDisk="true"/>   
   <cache
    name="org.hibernate.cache.UpdateTimestampsCache"
    maxElementsInMemory="5000"
    eternal="true"
    overflowToDisk="true"/>
</ehcache>
