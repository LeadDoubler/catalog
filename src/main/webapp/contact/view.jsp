<%@ include file="/taglibs.jsp"%>
<stripes:layout-render name="/layout/contact.jsp" title="Kontakt">
    <stripes:layout-component name="contents">
        <table>
            <tr>
                <td>Dato :</td>
                <td><fmt:formatDate value="${actionBean.contact.time}" type="both" pattern="HH:mm-dd.MM.yy" /></td>
            </tr>
            <tr>
                <td>Navn :</td>
                <td>${actionBean.contact.name}</td>
            </tr>
            <tr>
                <td>Telefon :</td>
                <td>${actionBean.contact.phone}</td>
            </tr>
            <tr>
                <td>Email :</td>
                <td>${actionBean.contact.email}</td>
            </tr>
            <tr>
                <td>SMS status :</td>
                <td>${actionBean.contact.result}</td>
            </tr>
            <tr>
                <td>Besked :</td>
                <td>${actionBean.contact.message}</td>
            </tr>
        </table>
    </stripes:layout-component>
</stripes:layout-render>


